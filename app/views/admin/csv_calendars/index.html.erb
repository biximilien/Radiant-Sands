<h1>Listing csv_calendars</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% csv_calendars.each do |csv_calendar| %>
      <tr>
        <td><%= link_to 'Show', admin_csv_calendar_path(csv_calendar) %></td>
        <td><%= link_to 'Edit', edit_admin_csv_calendar_path(csv_calendar) %></td>
        <td><%= link_to 'Destroy', admin_csv_calendar_path(csv_calendar), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Csv calendar', new_csv_calendar_path %>
